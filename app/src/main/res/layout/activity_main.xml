<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="id.ac.itb.digim.view.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Equalisasi"
        android:id="@+id/eqButton"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="67dp"
        android:onClick="toEqualization"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chain Code"
        android:id="@+id/chainButton"
        android:layout_below="@+id/eqButton"
        android:layout_centerHorizontal="true"
        android:onClick="toChainCode"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Merk Mobil"
        android:id="@+id/button"
        android:layout_below="@+id/chainButton"
        android:layout_alignLeft="@+id/chainButton"
        android:layout_alignStart="@+id/chainButton"
        android:onClick="toMerkMobil"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deteksi Angka v2"
        android:id="@+id/angkaV2"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true"
        android:onClick="toAngkaV2"/>
</RelativeLayout>
